{

  "manifest_version": 3,
  "name": "Redirectify",
  "version": "0.4.0",

  "description": "Redirects requests for PDFs of papers at some popular sites to their corresponding HTML index pages.",

  "homepage_url": "https://github.com/imurray/redirectify",

  "icons": {
    "128": "r.png"
  },

  "declarative_net_request": {
    "rule_resources": [{
      "id": "redirectify_ruleset",
      "enabled": true,
      "path": "dnr_rules.json"
    }]
  },

  "permissions": [
    "declarativeNetRequest"
  ],

  "host_permissions": [
    "*://*.arxiv.org/*",
    "*://xxx.lanl.gov/*",
    "*://arxiv-export-lb.library.cornell.edu/*",
    "*://arxiv.org/*",
    "*://openreview.net/*",
    "*://proceedings.mlr.press/*",
    "*://*.jmlr.org/*",
    "*://jmlr.csail.mit.edu/*",
    "*://dl.acm.org/*",
    "*://dlnext.acm.org/*",
    "*://papers.nips.cc/*",
    "*://papers.neurips.cc/*",
    "*://proceedings.nips.cc/*",
    "*://proceedings.neurips.cc/*",
    "*://pdfs.semanticscholar.org/*",
    "*://www.biorxiv.org/*",
    "*://www.medrxiv.org/*",
    "*://indico.cern.ch/*",
    "*://indico.fnal.gov/*",
    "*://cds.cern.ch/*",
    "*://www.jstor.org/*",
    "*://*.projecteuclid.org/*",
    "*://www.researchgate.net/*",
    "*://citeseer.ist.psu.edu/*",
    "*://citeseerx.ist.psu.edu/*",
    "*://www.aclweb.org/*"
  ]

}
